html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video{
	margin: 0;
	padding: 0;
}
ol,ul{
	list-style: none;
}
body,html{
	width:100%;
	height:100%;
	box-sizing: border-box;
	font-family: 'microsoft yahei','\534E\6587\7EC6\9ED1',Verdana,Arial,Helvetica,sans-serif;
	background:url(../img/mianBg.png) no-repeat ;
}
*{
	color: #FFF;
}
nav{
	width: 1920px;
	height: 79px;
	background-color: #101d31;
	background: rgba(0,0,0,0.84);
}
.messageLog,.navigation,.navigation>li,.SmartPlan>li,.userLogin,.userLogin>div{
	float: left;
}
.messageLog{
	color: #ffffff;
	width: 390px;
	height: 100%;
	margin-left: 37px;
	font-size: 32px;
	line-height: 79px;
}
.messageLog>img{
	vertical-align: middle;
	margin-right: 27px;
}
/*导航条*/
.navigation{
	line-height: 79px;
	margin-left: 17.5px;
}
.navigation>li{
	width: 109px;
	font-size: 26px;
	color: #ffffff;
	position: relative;
	padding: 0 24.5px;
	text-align: center;
}
.navigation a{
	text-decoration: none;
	font-size: 24px;
	color: #b0b2b4;
}
.navigation>li>p{
	width: 100%;
	height: 79px;
	font-size: 26px;
}
.SmartPlan{
	width: 783px;
	height: 56px;
	position: absolute;
	background-color: #101d31;
	top:79px;
	left: -200px;
	float: left;
	z-index: 9999;
	line-height: 56px;
	background: rgba(0,0,0,0.88);
}
/*点击之后字体发生改变*/
.n_col{
 	color: #89b1ef !important;
}
.SmartPlan>li{
	padding-left: 47px;
	float: left;
	margin-left: 35px;
}
.SmartPlan>li:nth-child(1){
	background: url(../img/web/空楼.png) no-repeat 5px 18px;
}
.SmartPlan>li:nth-child(2){
	background: url(../img/0171214173411.png) no-repeat 7px 18px;
}
.SmartPlan>li:nth-child(3){
	background: url(../img/web/人员.png) no-repeat 8px 19px;
}
.SmartPlan>li:nth-child(4){
	background: url(../img/web/选中衍生.png) no-repeat 8px 20px;
}
.userLogin{
	float: right;
}
.userLogin>div{
	float: left;
}
.question{
	width: 37px;
	height: 37px;
	margin-top: 20px;
	background: url(../img/帮助.png)no-repeat;
	opacity: 0.7;
}
.userJpg{
	width: 39px;
	height: 39px;
	background: url(../img/用户拷贝.png)no-repeat;
	background-size:39px 39px ;
	margin-top: 19px;
	margin-right: 10px;
	margin-left:25px ;
}
.userName{
	width: 108px;
	color: #cacbcd;
	line-height: 79px;
	
}
.onOff{
	float: right;
	width: 93px;
	height: 79px;
	background: url(../img/onOff_03.png) no-repeat;
}

/*主体内容*/
.subject{
	width: 100%;
	height: calc(100% - 79px);
	position: relative;
}

/*底部选项卡*/
.baseOption{
	height: 43px;
	background-color: #303b48;
	position: absolute;
	bottom: 32px;
	left: 198px;
	border-radius: 3px 0px 0px 3px;
	z-index: 999;
}
.baseOption>ul>li{
	float: left;
	height: 43px;
	width: 129px;
	text-align: center;
	line-height: 43px;
	margin-left: 28px;
	font-size: 20px;
}
.b_show{
	width: 129px;
	position: absolute;
	bottom: 100%;
}
.b_show>p{
	width: 129px;
	height: 34px;
	background: url(../img/多边形1拷贝4.png) no-repeat;
	line-height: 34px;
	position: absolute;
	top: -32px;
	left: -1px;
	z-index: 10;
}
.b_show>p>img{
	margin-top: 5px;
}
.b_show>ul{
	width: 125px;
	background: #fff;
	border-left:1px solid #507496 ;
	border-right:1px solid #507496 ;
	position: relative;
	z-index: 15;
	padding: 5.5px 0;
	padding-top:8px ;
	display: none;
}
.b_show>ul>li{
	height: 20px;
	line-height: 20px;
    margin-bottom: 8px;
    text-align: left;
    box-sizing: border-box;
    padding-left: 25px;
    background: no-repeat 4px 4px;
}

.b_show>ul>li>div{
	float: left;
}
.b_show>ul>li>div:nth-child(1){
	width: 78px;
	font-size: 16px;
    color: #1b4d96;
}
.b_show>ul>li>div:nth-child(2){
	width: 13px;
	float: right;
	margin-right: 8px;
}
/*清除*/
.eliminate{
	color: #8ca8d2;
	width: 113px;
	box-sizing: border-box;
	padding-left: 29px;
	background: url(../img/右侧icon/web/清除.png) no-repeat 5px 8px;
	z-index: 999;
	cursor: pointer;
	display: none;
}

/*侧边栏*/
.r_sidebar{
	width: 473px;
    background-color: #303b48;
    float: right;
    z-index: 99999;
    position: absolute;
    right: 10px;
    bottom: 859px;
    border-radius: 3px;
    background: rgba(0,0,0,0.8);
}
.r_sidebar_seek{
	width: 100%;
	border-radius: 3px;
	height: 63px;
	line-height: 63px;
}
/*筛选范围*/
.choose{
	width: 144px;
	height: 100%;
	float: left;
	border-right: 1px solid #525b66;
	position: relative;
}
.choose,.choose_l,.choose_r{
	float: left;
}
.choose_l{
	width: 103px;
	height: 100%;
	line-height: 63px;
	text-align: center;
	font-size: 24px;
}
.choose_r{
	width:22px;
	height: 13px;
	background: url(../img/下拉.png) no-repeat;
	margin-top: 25px;
}
.choose_l_spread{
	background-color: #303b48;
	position:absolute ;
	width: 100%;
	height: 126px;
	border-right: 1px solid #525b66;
	top: 100%;
	display: none;
	z-index: 9999;
	background: rgba(0,0,0,0.8);
}
.choose_l_spread>div{
	height: 63px;
	width: 100%;
	border-top: 1px solid #525b66;
	box-sizing: border-box;
	padding-right: 41px;
}
/*搜索框*/
.hunt{
	float: right;
	height: 63px;
	width: 328px;
	position: relative;
}
.huntFrame{
	width: 188px;
	outline: none;
	height: 63px;
	border: none;
	padding: 0px;
	background: none;
	padding-left: 31px;
	font-size: 20px;
	float: left;
}
.huntIcon{
	float: left;
	width: 30px;
	height: 30px;
	background: url(../img/搜索.png) no-repeat;
	margin-top: 19px;
	margin-left: 44px;
	cursor: pointer;
}

/*搜索展示区域*/
.Recommended{
	width: 473px;
	height: 665px;
	background-color: #303b48;
	background: rgba(0,0,0,0.8);
	z-index: 9999;
	position: absolute;
	right: 10px;
	bottom: 187px;
	border-radius: 5px 0px 0px 5px;
	box-sizing: border-box;
	padding-left: 10px;
}
.Recommended>div:nth-child(1){
	height: 73px;
	width: 447px;
	line-height: 73px;
	font-size: 26px;
	letter-spacing: 5px;
	text-align: center;
}
.fileOptions{
	width: 447px;
	opacity: 0.9;
}
.fileOptions>li{
	box-sizing: border-box;
	padding-left: 17px;
	padding-right: 23px;
	border-top: rgb(159, 175, 200) 1px solid;
	margin-bottom: 10pxs;
}
.genre{
	height: 52px;
	line-height: 52px;
	letter-spacing: 2px;
	color: #89b1ef;
	font-size: 26px;
	margin-top: 3px;
}
.genreDetails{
	height: 70px;
	margin-bottom: 15px;
}
/*改变底部选项卡复选框默认样式*/
.baseOption input[type="checkbox"]{
	-webkit-appearance: none;
	width: 13px;
	height: 13px;
	vertical-align: middle;
	box-sizing: border-box;
	border: 1px solid #959595;
	border-radius: 2px;
	outline: none;
}
.baseOption input[type="checkbox"]:checked{
	background: url(../img/b_checkbox_03.png) no-repeat;
	border: none;
}
.sub_html_style{
	color: #FFFFFF !important;
    font-size: 26px !important;
}
