
//底部移入效果

//var flag = false;
$(".baseOption>ul>li").mouseover(function() {
	$(this).find('ul').show();
});
$(".baseOption>ul>li").mouseout(function() {
	$(this).find('ul').hide();
});

//地理信息展示
$(document).on('click',".strike",function(){
	$(this).toggleClass("changeLevel");
	$(this).parent(".circuit").siblings(".StartNavigation").toggleClass("show");
})
//搜索数据框
$(".click_show>div").click(function(){
	$(".minute_path").toggleClass("hide");
	$(this).toggleClass("harvest");
})

//收起
$(".collect").click(function(){
	$(this).toggleClass("conceal");
	$(".evacuate_programme").toggleClass("le")
})

//搜索筛选范围
$("#choose_l").click(function(){
	$(".choose_l_spread").show()
})

$(".choose_l_spread>div").click(function(){
	$("#choose_l").text($(this).text());
	$(".choose_l_spread").hide()
})