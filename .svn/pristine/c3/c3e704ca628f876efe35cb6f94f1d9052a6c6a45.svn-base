/* checkbox */


.sendPlan .conter .table .checkbox {
  width: 320px;
  height: 24px;
  box-sizing:border-box;
  position: relative;
}

.sendPlan .conter .table .checkbox input {
	display: inline-block;
	width: 24px;
	height: 24px;
	top: -4px;
	left: 20px;
	position: absolute;
}


/* 超出部分隐藏且出现省略号 */
.sendPlan .conter .table .checkbox label {
	width: 300px;
	height: 40px;
	top: -10px;
	left: 24px;
	position: absolute;
	overflow: hidden; /*自动隐藏文字*/
	text-overflow: ellipsis;/*文字隐藏后添加省略号*/
	white-space: nowrap;/*强制不换行*/
	width: 20em;/*不允许出现半汉字截断*/
}

/* 鼠标移入显示隐藏文版 */
.sendPlan .conter .table .checkbox label:hover {
	white-space:normal;
	overflow:visible;
	/* color: orange; */
}

.sendPlan .conter .table .tab-cont {
	width: 1400px;
	height: 780px;
	overflow-x: hidden; 
	overflow-y: auto; 
}

.sendPlan .conter .table>thead>tr>th {
	height: 44px;
	line-height: 44px;
	text-align: left;
	padding-left: 22px;
	font-size: 22px;
	color: #c1c1c1;
	background-color: #050e1b;
}

.sendPlan .conter .table>tbody>tr>td {
	height: 44px;
	line-height: 44px;
	text-align: left;
	padding-left: 22px;
	font-size: 22px;
	color: #fff;
}

/* 隔行变色 */
.sendPlan .conter .table>tbody>tr:nth-child(odd) {
	background-color: #0b1e3b;
}

.sendPlan .conter .table>tbody>tr:nth-child(even) {
	background-color: #050e1b;
}


/* 发送按钮 */
.sendPlan .conter .list .list-send {
	width: 1400px;
	height: 76px;
	position: relative;
}

.sendPlan .conter .list .list-send a {
	width: 126px;
