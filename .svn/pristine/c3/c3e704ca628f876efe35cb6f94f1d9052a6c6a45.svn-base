	.sendPlan .member-show::-webkit-scrollbar-track {
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.28);  
		background: #fff ;  
	}  
	/*定义滑块 内阴影+圆角*/  
	.sendPlan .member-show::-webkit-scrollbar-thumb {
		border-radius: 3px;  
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
		background-color:rgba(53, 58, 60, 1);  
	}  
	.sendPlan .member-show::-webkit-scrollbar-thumb:hover {
		border-radius: 3px;  
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
		background-color:rgba(53, 58, 60, 1);  
}  
