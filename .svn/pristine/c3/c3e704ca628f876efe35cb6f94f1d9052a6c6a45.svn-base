/*滚动条 start*/
	/* table表格 */
	.sendPlan .list .tab-cont::-webkit-scrollbar {
		width: 10px;  
		height: 4px;  
		background-color: #F5F5F5;  
	}  
	/*定义滚动条轨道 内阴影+圆角*/  
	.sendPlan .list .tab-cont::-webkit-scrollbar-track {
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.28);  
		background: #fff ;  
	}  
	/*定义滑块 内阴影+圆角*/  
	.sendPlan .list .tab-cont::-webkit-scrollbar-thumb {
		border-radius: 3px;  
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
		background-color:rgba(53, 58, 60, 1);  
	}  
	.sendPlan .list .tab-cont::-webkit-scrollbar-thumb:hover {
		border-radius: 3px;  
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
		background-color:rgba(53, 58, 60, 1);  
}  

