.wrapper {
	width: 1920px;
	height: 1079px;
	box-sizing:border-box;
	background: url(../../img/sms/bgcImage.jpg) no-repeat;
	overflow: hidden;
}

/*滚动条 start*/
	/* 首页预案 */
	.smartPlan .curPlan::-webkit-scrollbar {
		width: 10px;  
		height: 4px;  
		background-color: #F5F5F5;  
	}  
	/*定义滚动条轨道 内阴影+圆角*/  
	.smartPlan .curPlan::-webkit-scrollbar-track {
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.28);  
		background: #fff ;  
	}  
	/*定义滑块 内阴影+圆角*/  
	.smartPlan .curPlan::-webkit-scrollbar-thumb {
		border-radius: 3px;  
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
		background-color:rgba(53, 58, 60, 1);  
	}  
	.smartPlan .curPlan::-webkit-scrollbar-thumb:hover {
		border-radius: 3px;  
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
		background-color:rgba(53, 58, 60, 1);  
}  

	/* 首页参与成员 */
	.smartPlan .member-show::-webkit-scrollbar {
		width: 10px;  
		height: 4px;  
		background-color: #F5F5F5;  
	}  
	/*定义滚动条轨道 内阴影+圆角*/  
	.smartPlan .member-show::-webkit-scrollbar-track {
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.28);  
		background: #fff ;  
	}  
	/*定义滑块 内阴影+圆角*/  
	.smartPlan .member-show::-webkit-scrollbar-thumb {
		border-radius: 3px;  
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
		background-color:rgba(53, 58, 60, 1);  
	}  
	.smartPlan .member-show::-webkit-scrollbar-thumb:hover {
		border-radius: 3px;  
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
		background-color:rgba(53, 58, 60, 1);  
}  

	/* 文本区 */
	.smartPlan .curPlace-news::-webkit-scrollbar {
		width: 10px;  
		height: 4px;  
		background-color: #F5F5F5;  
	}  
	/*定义滚动条轨道 内阴影+圆角*/  
	.smartPlan .curPlace-news::-webkit-scrollbar-track {
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.28);  
		background: #fff ;  
	}  
	/*定义滑块 内阴影+圆角*/  
	.smartPlan .curPlace-news::-webkit-scrollbar-thumb {
		border-radius: 3px;  
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
		background-color:rgba(53, 58, 60, 1);  
	}  
	.smartPlan .curPlace-news::-webkit-scrollbar-thumb:hover {
		border-radius: 3px;  
		-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
		background-color:rgba(53, 58, 60, 1);  
}  




/* 顶部公共区域 */
.header {
	width: 100%;
	height: 79px;
	background-color: #0d192d;
}

/* 内容部分 */
.page {
	width: 100%;
	height: 1000px;
	box-sizing:border-box;
	position: relative;
}
/* 当前预案 */
.smartPlan .curPlan {
	width: 460px;
	height: 988px;
	border:1px solid #126062;
	border-radius: 4px;
	box-sizing:border-box;
	margin-left: 16px;
	margin-top: 12px;

	overflow-x:hidden;
	overflow-y:auto;


	background-color: #050e1a;
}
/* 标题 */
.smartPlan .curPlan-title {
	width: 434px;
	height: 60px;
	background-color: #0b2344;
	color: #fff;
	font-size: 24px;
	line-height: 60px;
	padding-left: 26px;
	margin-top: 20px;
}
#le{
	text-align: left !important;
}
/* 搜索框 */
.smartPlan .curPlan-search {
	width: 460px;
	height: 60px;
	background-color: #050e1c;
	padding:9px 14px 0 26px;
	box-sizing:border-box;
}

/* 搜索图标 */
.smartPlan i.search {
	width: 30px;
	height: 30px;
	background: url(../../img/sms/icon_search.png) no-repeat;
	display: block;
	float: left;
	margin-top: 6px;
}

.smartPlan .curPlan-search input {
	width: 280px;
	height: 42px;
	color: #929498;
	font-size: 20px;
	float: left;
	margin-left: 20px;
}

.smartPlan .curPlan-search span {
	width: 88px;
	height: 42px;
	line-height: 42px;
	background-color: #1a4c95;
	color: #fff;
	font-size: 20px;
	text-align: center;
	float: right;
	border-radius: 4px;
}

/* 导航条部分 */

.smartPlan .curPlan-nav {
	width: 460px;
	background-color: #050d19;
}

.smartPlan .curPlan-nav .navTitle {
	width: 460px;
}

.smartPlan .curPlan-nav .nav:hover .navTitle,.smartPlan .curPlan-nav .nav .navTitle.active {
	background-color: #114591;
}
.smartPlan .curPlan-nav .on {
	background-color: #114591;

}
.smartPlan .curPlan-nav .navTitle a {
	display: block;
	float: left;
	width: 300px;
	height: 60px;
	line-height: 60px;
	padding-left: 26px;
	font-size: 20px;
	color: #fff;

}
/* 下拉按钮图标 */
.smartPlan i.pulldown {
	float:right;
	width: 22px;
	height: 22px;
	margin-top: 19px;
	margin-right: 24px;
	display: block;
	background: url(../../img/sms/icon_pulldown.png) no-repeat;
}

/* 右拉按钮图标 */
.smartPlan i.pullright {
	float:right;
	width: 22px;
	height: 22px;
	margin-top: 19px;
	margin-right: 24px;
	display: block;
	background: url(../../img/sms/icon_pullright.png) no-repeat;
}

/* 子导航条 */
.smartPlan .curPlan-nav .subnav li {
	width: 398px;
	height: 60px;
	line-height: 60px;
	color: #fff;
	font-size: 20px;
	padding-left: 62px;
}

.smartPlan .curPlan-nav .subnav li:hover {
	background-color: #0a254e;
}
/* 当前预案结束 */

/* 当前位置 */

.smartPlan .curPlace {
	width: 1400px;
	height: 988px;
	background-color: #0d192d;
	margin-top: 12px;
	margin-left: 12px;
	border:1px solid #126062;
	border-radius: 4px;
	box-sizing:border-box;
}

.smartPlan .curPlace-title {
	width: 100%;
	height: 60px;
	line-height: 60px;
	background-color: #0b2344;
	font-size: 24px;
	padding-left: 26px;
	box-sizing:border-box;
}
.smartPlan .curPlace-left {
	width: 100%;
	height: 60px;
	position: relative;
}

.smartPlan .curPlace-left span {
	color: #fff;
}

.smartPlan .curPlace-left .look {
	color: #fff;
	text-decoration: underline;
}

.smartPlan .curPlace-left .distribute {
	width: 126px;
	height: 50px;
	line-height: 50px;
	display: inline-block;
	color: #fff;
	background-color: #1b509a;
	border-radius: 5px;
	text-align: center;
	position: absolute;
	top: 5px;
	right: 70px;
}

/* 文档内容 */

.smartPlan .content .w862 {
	width: 870px;
	height: 427px;
	background: url(../../img/sms/8-6-2.png);
	margin-left: 58px;
}

.smartPlan #curPlace-content img {
	margin-left: 58px;
}

.smartPlan .curPlace-news {
	padding: 0 12px;
	width: 100%;
	height: 926px;
	line-height: 1.5;
	box-sizing:border-box;
	overflow-x:hidden;
	overflow-y:auto;
}

.smartPlan .curPlace-news .title {
	width: 100%;
	height: 116px;
	text-align: center;
	margin-top: 50px;
}

.smartPlan .curPlace-news .content ul {
	margin-bottom: 30px;
}

.smartPlan .curPlace-news .content li {
	margin-bottom: 30px;
}

.smartPlan .curPlace-news h2 a {
	margin-top: 15px;
	color: #fff;
	font-size: 24px;
}

.smartPlan .curPlace-news h3 {
	color: #cce0ff;
	font-size: 24px;
	margin-top: 27px;
}

.smartPlan .curPlace-news h4 {
	color: #ccc;
	font-size: 24px;
	margin-top: 27px;
}

.smartPlan .curPlace-news p {
	color: #fff;
	font-size: 22px;
}

/* 当前位置结束 */


/* 参与成员 */

.smartPlan .member {
	position: absolute;
	top: 12px;
	left: -460px;
}

/* 解决滑出抖动bug */
.smartPlan .member-bug {
	width: 100px;
	height: 1000px;
	position: absolute;
	top: 12px;
