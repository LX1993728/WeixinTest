/* 流程图 */

.smartPlan .curProcess-show {
	width: 500px;
	height: 1000px;
	position: absolute;
	top: 12px;
	right: 0;
	background-color: #060d18;
}

.smartPlan .curProcess-show .curProcess-icon {
	width: 80px;
	height: 50px;
	border-radius: 5px 0 0 5px;
	background-color: #1763a2;
	position: absolute;
	top: 6px;
	left: -80px;
	padding: 15px 6px;
	box-sizing:border-box;
}
.smartPlan .curProcess-show .curProcess-icon:hover {
	cursor:pointer;
}

