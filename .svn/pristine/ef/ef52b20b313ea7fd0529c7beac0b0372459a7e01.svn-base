.s-box{
	width:1870px;
	height:785px;
}
.s-boxLeft{
	float: left;
	width: 1361px;
	height: 785px;
	border-radius: 5px;
	border: solid 1px #166b6b;
}
.s-boxLeftTitle{
	width:1361px;
	border-radius:5px 5px 0 0;
}
.tab{
	width:1361px;
	height:61px;
	/*background-color: #155abf;
	opacity: 0.25;*/
	background: rgba(21,90,191,0.25);
}
.tab>a{
	font-size: 27px;
	color:#fff;
	display:block;
	width:142px;
	height:56px;
	float: left;
	line-height: 56px;
	text-align: center;
	margin-left:8px;
}
.tab a.on{border-bottom:5px solid #92bdff;}
.content{overflow:hidden;}
.contentBox{
	padding:38px 47px 0px 26px;
	height:685px;
}
.contentBox>li{
	display: none;
}
.contentBox>li.on{
	display: block;
}
.content li>p{
	font-size: 24px;
	line-height:30px;
	color:#ededed;
}

.s-info{
	position:fixed;
	width:780px;
	height:50px;
	background: #f00;
	font-size: 16px;
	color:#ccc;
	margin-left:290px;
	bottom:200px;
	line-height:24px;
}
.s-boxRight{
    float: right;
}
.s-boxTop{
	float: right;
	width: 498px;
	height: 471px;
	border-radius: 5px;
	border: solid 1px #146062;
	margin-bottom:10px;
}
.s-sboxTop{
	padding-bottom: 17px;
	width: 498px;
	height:393px;
	overflow: auto;
}
.s-boxTopTitle{
	width:477px;
	height:61px;
	line-height: 61px;
	font-size: 27px;
	color:#fff;
    padding-left:19px;
	background: rgba(21,90,191,0.25);
}
.s-boxRight1{
	overflow: hidden;
	width:470px;
	position: relative;
	min-height:66px;
	padding:17px 9px 0px 9px;
	height:auto;
}
.s-boxRightnum{
	padding-left:5px;
	display: inline-block;
	float: left;
	width: 13px;
	height: 25px;
	font-family: FZLTZHK--GBK1-0;
	font-size: 30px;
	font-weight: normal;
	font-style: italic;
	font-stretch: normal;
	line-height: 48px;
	letter-spacing: 2px;
	color: #eaeaea;
}
.s-boxRighttext{
	padding:19px 18px 23px 18px;
	float: right;
	width: 396px;
	border-radius: 7px;
	font-size: 20px;
	color: #cce0ff;
	letter-spacing:1.5px;
	line-height:24px;
	background: rgba(21,90,191,0.25);
}
.s-boxRightBottom{
	clear: both;
	width: 498px;
	height: 304px;
	border-radius: 5px;
	border: solid 1px #146062;
}
.s-boxRightBotCon{
	padding-top:18px;
	padding-right:42px;
	padding-left:24px;
	font-size: 20px;
	height: 226px;
	overflow-x:hidden;
	overflow-y:auto;
	line-height:24px;
	letter-spacing:1.5px;
	color: #cce0ff;	
}
.s-tabInfo{
	position: relative;
	width:960px;
	height:539px;
	margin:auto;
} 
.s-slideShowTell{
	padding-top:22px;
	padding-right: 29px;
	padding-left:29px;
	width: 1306px;
	height: 112px;
	position: fixed;
	bottom:30px;
	left:0;
	font-size: 22px;
	font-weight: normal;
	font-stretch: normal;
	line-height:30px;
	letter-spacing: 1px;
	color: #ffffff;
	background: rgba(21,90,191,0.25);
	border-top:1px solid #154285;
}
/*滚动条*/
.s-sboxTop::-webkit-scrollbar {
	width: 10px;  
	height: 0px;  
	background-color: #F5F5F5;  
	}  
/*定义滚动条轨道 内阴影+圆角*/  
.s-sboxTop::-webkit-scrollbar-track {
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.28);  
	background: #6f747c ;  
}  
/*定义滑块 内阴影+圆角*/  
.s-sboxTop::-webkit-scrollbar-thumb {
	border-radius: 3px;  
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
	background-color:rgba(53, 58, 60, 1);  
	}  
.s-sboxTop::-webkit-scrollbar-thumb:hover {
	border-radius: 3px;  
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
	background-color:rgba(53, 58, 60, 1);  
}
