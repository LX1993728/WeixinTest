html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video{
	margin: 0;
	padding: 0;
}
ol,ul{
	list-style: none;
}
body,html{
	width:100%;
	height:100%;
	box-sizing: border-box;
	font-family: 'microsoft yahei','\534E\6587\7EC6\9ED1',Verdana,Arial,Helvetica,sans-serif;
	background:url(../img/mianBg.png) no-repeat ;
}
*{
	color: #FFF;
}
nav{
	width: 1920px;
	height: 79px;
	background-color: #101d31;
	background: rgba(0,0,0,0.84);
}
.messageLog,.navigation,.navigation>li,.SmartPlan>li,.userLogin,.userLogin>div{
	float: left;
}
.messageLog{
	color: #ffffff;
	width: 390px;
	height: 100%;
	margin-left: 37px;
	font-size: 32px;
	line-height: 79px;
}
.messageLog>img{
	vertical-align: middle;
	margin-right: 27px;
}
/*导航条*/
.navigation{
	line-height: 79px;
	margin-left: 17.5px;
}
.navigation>li{
	width: 109px;
	font-size: 26px;
	color: #ffffff;
	position: relative;
	padding: 0 24.5px;
	text-align: center;
}
.navigation a{
	text-decoration: none;
	font-size: 24px;
	color: #b0b2b4;
}
.navigation>li>p{
	width: 100%;
	height: 79px;
	font-size: 26px;
}
.SmartPlan{
	width: 783px;
	height: 56px;
	position: absolute;
	background-color: #101d31;
	top:79px;
	left: -200px;
	float: left;
	z-index: 9999;
	line-height: 56px;
	background: rgba(0,0,0,0.88);
}
/*点击之后字体发生改变*/
.n_col{
 	color: #89b1ef !important;
}
.SmartPlan>li{
	padding-left: 47px;
	float: left;
	margin-left: 35px;
}
.SmartPlan>li:nth-child(1){
	background: url(../img/web/选中空楼.png) no-repeat 5px 18px;
}
.SmartPlan>li:nth-child(2){
	background: url(../img/0171214173411.png) no-repeat 7px 18px;
}
.SmartPlan>li:nth-child(3){
	background: url(../img/web/人员.png) no-repeat 8px 19px;
}
.SmartPlan>li:nth-child(4){
	background: url(../img/web/衍生.png) no-repeat 8px 20px;
}
.userLogin{
	float: right;
}
.userLogin>div{
	float: left;
}
.question{
	width: 37px;
	height: 37px;
	margin-top: 20px;
	background: url(../img/帮助.png)no-repeat;
	opacity: 0.7;
}
.userJpg{
	width: 39px;
	height: 39px;
	background: url(../img/用户拷贝.png)no-repeat;
	background-size:39px 39px ;
	margin-top: 19px;
	margin-right: 10px;
	margin-left:25px ;
}
.userName{
	width: 108px;
	color: #cacbcd;
	line-height: 79px;
	
}
.onOff{
	float: right;
	width: 93px;
	height: 79px;
	background: url(../img/onOff_03.png) no-repeat;
}

/*主体内容*/
.subject{
	width: 100%;
	height: calc(100% - 79px);
	position: relative;
}

/*侧边栏*/
.r_sidebar{
	width: 473px;
    background-color: #303b48;
    float: right;
    z-index: 99999;
    position: absolute;
    right: 10px;
    top: 153px;
    border-radius: 3px;
    background: rgba(0,0,0,0.8);
}
.r_sidebar_seek{
	width: 100%;
	border-radius: 3px;
	height: 63px;
	line-height: 63px;
}
/*筛选范围*/
.choose{
	width: 144px;
	height: 100%;
	float: left;
	border-right: 1px solid #525b66;
	position: relative;
}
.choose,.choose_l,.choose_r{
	float: left;
}
.choose_l{
	width: 103px;
	height: 100%;
	line-height: 63px;
	text-align: center;
	font-size: 24px;
}
.choose_r{
	width:22px;
	height: 13px;
	background: url(../img/下拉.png) no-repeat;
	margin-top: 25px;
}
.choose_l_spread{
	position:absolute ;
	width: 100%;
	height: 126px;
	border-right: 1px solid #525b66;
	top: 100%;
	display: none;
	z-index: 9999;
	background-color: #303b48;
	 background: rgba(0,0,0,0.8);
}
.choose_l_spread>div{
	height: 63px;
	width: 100%;
	border-top: 1px solid #525b66;
	box-sizing: border-box;
	padding-right: 41px;
}
/*筛选框点击换图片*/
.filtrate{
	background: url(../img/上拉.png) no-repeat;
}
/*搜索框*/
.hunt{
	float: right;
	height: 63px;
	width: 328px;
	position: relative;
}
.huntFrame{
	width: 188px;
	outline: none;
	height: 63px;
	border: none;
	padding: 0px;
	background: none;
	padding-left: 31px;
	font-size: 20px;
	float: left;
}
.huntIcon{
	float: left;
	width: 30px;
	height: 30px;
	background: url(../img/搜索.png) no-repeat;
	margin-top: 19px;
	margin-left: 44px;
	cursor: pointer;
}

/*右边的搜索展示*/
.r_show{
	width: 473px;
	height: 466px;
	box-sizing: border-box;
	padding: 10px;
	position: absolute;
	top: 70px;
	border-radius: 3px;
	background-color: #303b48;
	padding-bottom: 10px;
	/*overflow: hidden;*/
	overflow-y: auto;
	background: rgba(0,0,0,0.8);
}
.headline{
	width: 447px;
	height: 48px;
	line-height: 48px;
	text-align: center;
	border-bottom:1px solid #9fafc8;
	font-size: 26px;
}
/*展示列表详情*/
.topic{
	height: 48px;
	line-height: 48px;
	border-bottom:1px solid #9fafc8;
}
.topic>div{
	float: left;
	width:149px ;
	font-size: 26px;
	text-align: center;
}
.r_show>ul{
	width: 447px;
}
.topic>div:nth-child(1){
	width: 183px;
	box-sizing: border-box;
	height: 48px;
	padding-left: 60px;
	font-size: 28px;
	background: url(../img/web/场所.png) no-repeat 21px 11px ;
	text-align: left !important;
}
.topic>div:nth-child(3){
	font-size: 20px;
	height: 23px;
	text-decoration: underline;
	float: right;
	width: 48px;
	margin-top: 5px;
	margin-right: 5px;
}
.topic span{
	color: #cce0ff;
}
/*展示区域*/
.topic_bare{
	width: 447px;
	
	padding-top: 10px;
	padding-bottom: 10px;
	border-bottom:1px solid #9fafc8;
	display: none;
	box-sizing: border-box;
	padding-left: 19px;
}
.topic_bare>li{
	height: 48px;
	line-height: 48px;
}
.topic_bare>li>div{
	font-size: 18px;
	float: left;
}
.topic_bare>li>div:nth-child(1){
	width: 206px;
}
.topic_bare>li>div:nth-child(2){
	width: 178px;
}
.topic_bare>li>div:nth-child(3){
	width: 28px;
	height: 28px;
	background:url(../img/dingwei.png) no-repeat ;
	margin-top: 11px;
}
.person_icon{
	background-image:url(../img/web/法人办事.png) !important;
}
.ship_icon{
	background-image: url(../img/web/船.png) !important;
}
.spread>span{
	color: #FFFFFF;
}
/*点击后的效果*/
.show{
	display: block;
}
.hue{
	color: #cce0ff !important;
}

/*地图上浮起的位置详情*/
.orientate{
	border: solid 2px #83a9e4;
	height: 48px;
	line-height: 48px;
	text-align: center;
	padding: 0px 25px;
	background: #3a3b3c;
	letter-spacing: 2px;
}
.fixedPoint{
	width: 32px;
	height: 27px;
	background: url(../img/图层6.png) no-repeat;
}

/*左侧标绘工具*/
.plotting{
	width: 209px;
	height: 255px;
	background-color: #101d31;
	border-radius: 0px 5px 5px 0px;
	position: absolute;
	left: 0px;
	bottom: 296px;
	z-index: 9999;
	background:rgba(0,0,0,0.8) ;
}
.plotting>div{
	height: 50px;
	font-size: 20px;
	letter-spacing: 1px;
	line-height: 48px;
}
.plotting>div:nth-child(1),.plotting>div:nth-child(2){
	box-sizing: border-box;
	padding-left: 60px;
	background: url(../img/web/核心区标绘.png) no-repeat 14px 10px;
}
.plotting>div:nth-child(2){
	background: url(../img/web/五角星拷贝.png) no-repeat 14px 12px;
}

.plotting>div:nth-child(3),.plotting>div:nth-child(4){
	box-sizing: border-box;
	padding-left: 15px;
}
.controlZone,.caution{
	width: 65px;
	height: 32px;
	background-color: #162235;
	letter-spacing: 1px;
	text-align: center;
	margin-top: 8px;
	line-height: 32px;
	margin-left: 5px;
	margin-right: 5px;
	float: left;
	border: none;
	outline: none;
	font-size: 20px;
}
.controlZone_sire>div,.caution_sire>div{
	float: left;
}
.plotting_district{
	height: 55px;
	background-color: #101d31;
	border-radius: 0px 5px 5px 0px;
	box-sizing: border-box;
	padding-left: 82px;
	background: url(../img/web/gis_标绘工具.png) no-repeat 24px 11px;
	font-size: 22px;
	letter-spacing: 1px;
}
/*选中区域详情*/
.regionalInformation{
	width: 628px;
	height: 155px;
	background-color: #101d31;
	border-radius: 0px 5px 5px 0px;
	position: absolute;
	left: 0px;
	bottom: 131px;
	z-index: 9999;
	box-sizing: border-box;
	padding: 11px;
	background: rgba(0,0,0,0.8);
}
.regionalInformation>div{
	float: left;
	width: 271px;
	font-size: 20px;
	letter-spacing: 1px;
	height: 48px;
	line-height: 48px;
	padding-left:31px ;
}
.regionalInformation>div>span{
	font-size: 20px;
}
.regionalInformation>div:nth-child(1){
	background: url(../img/web/矩形9.png)no-repeat 0px 14px;
}
.regionalInformation>div:nth-child(2){
	background: url(../img/web/矩形9拷贝2.png)no-repeat 0px 14px;
}
.regionalInformation>div:nth-child(3){
	background: url(../img/web/矩形9拷贝.png)no-repeat 0px 14px;
}
.regionalInformation>div:nth-child(4){
	background: url(../img/web/五角星拷贝.png)no-repeat 0px 13px;
}
.mark{
	width: 555px;
	height: 48px;
	line-height:48px ;
}
.mark>li{
	float: left;
	width: 110px;
	height: 38px;
	line-height: 38px;
	box-sizing: border-box;
	font-size: 20px;
	padding-left: 47px;
}
.mark>li:nth-child(1){
	background: url(../img/web/多边形3拷贝6.png) no-repeat 0px 8px;
}
.mark>li:nth-child(2){
	background: url(../img/web/多边形3拷贝7.png) no-repeat 0px 8px;
}
.mark>li:nth-child(3){
	background: url(../img/执法船.png) no-repeat 0px 8px;
}
.mark>li:nth-child(4){
	background: url(../img/web/多边形3拷贝8.png) no-repeat 0px 8px;
}
.mark>li:nth-child(5){
	background: url(../img/web/多边形3拷贝9.png) no-repeat 0px 8px;
}
/*底部选项卡*/
.baseOption{
	height: 43px;
	background-color: #303b48;
	position: absolute;
	bottom: 32px;
	left: 198px;
	border-radius: 3px 0px 0px 3px;
	z-index: 999;
}
.baseOption>ul>li{
	float: left;
	height: 43px;
	width: 129px;
	text-align: center;
	line-height: 43px;
	margin-left: 28px;
	font-size: 20px;
}
.b_show{
	width: 129px;
	position: absolute;
	bottom: 100%;
}
.b_show>p{
	width: 129px;
	height: 34px;
	background: url(../img/多边形1拷贝4.png) no-repeat;
	line-height: 34px;
	position: absolute;
	top: -32px;
	left: -1px;
	z-index: 10;
}
.b_show>p>img{
	margin-top: 5px;
}
.b_show>ul{
	width: 125px;
	background: #fff;
	border-left:1px solid #507496 ;
	border-right:1px solid #507496 ;
	position: relative;
	z-index: 15;
	padding: 5.5px 0;
	padding-top:8px ;
	display: none;
}
.b_show>ul>li{
	height: 20px;
	line-height: 20px;
    margin-bottom: 8px;
    text-align: left;
    box-sizing: border-box;
    padding-left: 25px;
    background: no-repeat 4px 4px;
}

.b_show>ul>li>input{
	
}
.b_show>ul>li>div{
	float: left;
}
.b_show>ul>li>div:nth-child(1){
	width: 78px;
	font-size: 16px;
    color: #1b4d96;
}
.b_show>ul>li>div:nth-child(2){
	width: 13px;
	float: right;
	margin-right: 8px;
}
/*清除*/
.eliminate{
	color: #8ca8d2;
	width: 113px;
	box-sizing: border-box;
	padding-left: 29px;
	background: url(../img/右侧icon/web/清除.png) no-repeat 5px 8px;
	z-index: 999;
	cursor: pointer;
	display: none;
}

/*改变底部选项卡复选框默认样式*/
.baseOption input[type="checkbox"]{
	-webkit-appearance: none;
	width: 13px;
	height: 13px;
	vertical-align: middle;
	box-sizing: border-box;
	border: 1px solid #959595;
	border-radius: 2px;
	outline: none;
}
.baseOption input[type="checkbox"]:checked{
	background: url(../img/b_checkbox_03.png) no-repeat;
	border: none;
}


/*滚动条*/
.curPlan::-webkit-scrollbar {
	width: 10px;  
	height: 4px;  
	background-color: #F5F5F5;  
	}  
/*定义滚动条轨道 内阴影+圆角*/  
.curPlan::-webkit-scrollbar-track {
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.28);  
	background: #6f747c ;  
}  
/*定义滑块 内阴影+圆角*/  
.curPlan::-webkit-scrollbar-thumb {
	border-radius: 3px;  
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
	background-color:rgba(53, 58, 60, 1);  
	}  
.curPlan::-webkit-scrollbar-thumb:hover {
	border-radius: 3px;  
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);  
	background-color:rgba(53, 58, 60, 1);  
}  
.sub_html_style{
	color: #FFFFFF !important;
    font-size: 26px !important;
}
