            $(".option a").click(function(){
                var value=$(this).text();
                $(this).parent().siblings(".select_txt").text(value);
                $(this).parent().parent().siblings('.select_value').val(value);
                var $btnRights = $(this).parents('.exam-grade').siblings('.exam-btn').children('.btnRight');
                nextPage ($btnRights,value);
             })
	// 所有的题目盒子
	var $contents = $(".content");
	// 所有的上翻页按钮
	var $btnLefts = $(".btnLeft");
	
	var parmas =[];
	function nextPage ($btnRights,value){
		// 点击下一题时让页面向后翻页
		$btnRights.click(function(){
			var h = $(this).attr('index');
			var s = Number(h)+1;
			var conID = '#content'+h;
			$("#content"+h).hide('slow');
			$("#content"+s).show('slow');
			parmas[Number(h-1)] =parseInt(value)
		});
		// 点击上一题时让页面向前翻页
		$btnLefts.click(function(){
			var s = $(this).attr('index');
			var h = Number(s)+1;
			$("#content"+h).hide('slow');
			$("#content"+s).show('slow');
		});
	}


	//提交数据
	$("#content16").find('.mod_select').find('a').click(function(){
		var value=$(this).text();
