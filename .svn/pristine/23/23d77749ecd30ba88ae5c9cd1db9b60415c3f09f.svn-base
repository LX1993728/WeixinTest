				//        secondObj.name7 = '演练分析';
				secondObj.score7 = parmas[13] + parmas[14];
				//        secondObj.name8 = '资料整理';
				secondObj.score8 = parmas[15];
				$(".tabFirst").each(function (i, val) {
					var score = 'score' + (i + 1);
					$(this).siblings('p').children('em').html(firstObj[score])
				})
				$(".tabSec").each(function (i, val) {
					var score = 'score' + (i + 1);
					$(this).siblings('p').children('em').html(secondObj[score])
				})
				$('.ScoreBar td h6').each(function (i, val) {
					$(this).siblings('p').children('em').html(parmas[i]);
				})
				$('.ScoreBar tbody tr td:last-child').each(function (i) {
					$(this).html(parmas[i]);
				})
