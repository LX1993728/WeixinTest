	})

	function submitData(value){
		$('#submitData').unbind("click").click(function(){
				var totalNum = 0;
				parmas[15] = parseInt(value);
				//console.log(parmas)
				// 创建一个一级方案对应的数组
				var firstObj = {};
				//		 '演练准备';
				firstObj.score1 = parmas[0] + parmas[1] + parmas[2] + parmas[3] + parmas[4] + parmas[5];
				//		'演练实施';
				firstObj.score2 = parmas[6] + parmas[7] + parmas[8] + parmas[9] + parmas[10] + parmas[11] + parmas[12];
				//		'总结阶段';
				firstObj.score3 = parmas[13] + parmas[14] + parmas[15];
				// 创建一个二级方案对应的数组
				var secondObj = {};
				//         '演练组织';
				secondObj.score1 = parmas[0] + parmas[1];
				//        secondObj.name2 = '演练方案';
				secondObj.score2 = parmas[2] + parmas[3];
				//        secondObj.name3 = '演练保障';
				secondObj.score3 = parmas[4] + parmas[5];
				//        secondObj.name4 = '信息沟通';
				secondObj.score4 = parmas[6] + parmas[7] + parmas[8];
				//        secondObj.name5 = '应急响应';
				secondObj.score5 = parmas[9] + parmas[10] + parmas[11];
				//        secondObj.name6 = '舆情应对';
