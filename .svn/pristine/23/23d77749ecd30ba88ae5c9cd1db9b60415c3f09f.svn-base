					totalNum += parseInt(val);
				})
				$('#totalNum').html(totalNum);
				$('#content16').hide('slow');
				$('#content17').show('slow');

				console.log(parmas)
				$.ajax({
			 		url: '/statistic/statisticByScore',
			 		type: "post",
			 		dataType: 'JSON',
			 		data:{
			 			 params:JSON.stringify(parmas)
			 		},success:function(data){
			 			 console.info(data)
			 			 		
