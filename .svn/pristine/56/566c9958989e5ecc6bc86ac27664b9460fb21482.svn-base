package com.example.po.template;
/**
 * 演练阶段场景中间部分——自定义场景类型
 * @author liuxun
 *
 */

import java.util.List;

public class StageSceneMiddle extends StageSceneBase {
	private static final long serialVersionUID = 1L;
	private List<SceneTask> tasks; // 场景任务集合
	private String explain; // 场景说明
	private List<ImageText> imgts; // 媒体——信息 集合

	public List<SceneTask> getTasks() {
		return tasks;
	}

	public void setTasks(List<SceneTask> tasks) {
		this.tasks = tasks;
	}

	public String getExplain() {
		return explain;
	}

	public void setExplain(String explain) {
		this.explain = explain;
	}

	public List<ImageText> getImgts() {
		return imgts;
	}

	public void setImgts(List<ImageText> imgts) {
		this.imgts = imgts;
	}

}
