nav{
	width: 1920px;
	height: 79px;
	background-color: #101d31;
	background: rgba(0,0,0,0.84);
}
.messageLog,.navigation,.navigation>li,.SmartPlan>li,.userLogin,.userLogin>div{
	float: left;
}
.messageLog{
	color: #ffffff;
	width: 390px;
	height: 100%;
	margin-left: 37px;
	font-size: 32px;
	line-height: 79px;
}
.messageLog>img{
	vertical-align: middle;
	margin-right: 27px;
}
/*导航条*/
.navigation{
	line-height: 79px;
	margin-left: 17.5px;
	box-sizing: border-box;
}
.navigation>li{
/*	width: 109px;*/
	font-size: 26px;
	color: #ffffff;
	position: relative;
	padding: 0 24.5px;
	text-align: center;
}
.navigation a{
	text-decoration: none;
	font-size: 24px;
	color: #b0b2b4;
}
.navigation>li>p{
	width: 100%;
	height: 79px;
	font-size: 26px;
	cursor: pointer;
	margin-bottom: none !important;
}
.SmartPlan{
	width: 783px;
	height: 56px;
	position: absolute;
	background-color: #101d31;
	top:79px;
	left: -200px;
	float: left;
	z-index: 9999;
	line-height: 56px;
	background: rgba(0,0,0,0.88);
	display: none;
}
/*点击之后字体发生改变*/
/*.n_col{
 	color: #89b1ef;;
}*/
.SmartPlan>li{
	padding-left: 47px;
	float: left;
	margin-left: 35px;
}
.SmartPlan>li:nth-child(1){
	background: url(./../img/web/空楼.png) no-repeat 5px 18px;
}
.SmartPlan>li:nth-child(2){
	background: url(./../img/0171214173411.png) no-repeat 7px 18px;
}
.SmartPlan>li:nth-child(3){
	background: url(./../img/web/人员.png) no-repeat 8px 19px;
}
.SmartPlan>li:nth-child(4){
	background: url(./../img/web/衍生.png) no-repeat 8px 20px;
}
.userLogin{
	float: right;
}
.userLogin>div{
	float: left;
}
.question{
	width: 37px;
	height: 37px;
	margin-top: 20px;
	background: url(./../img/帮助.png)no-repeat;
	opacity: 0.7;
}
.userJpg{
	width: 39px;
	height: 39px;
	background: url(./../img/用户拷贝.png)no-repeat;
	background-size:39px 39px ;
	margin-top: 19px;
	margin-right: 10px;
	margin-left:25px ;
}
.userName{
	width: 108px;
	color: #cacbcd;
	line-height: 79px;
	font-size: 20px;
}
.onOff{
	float: right;
	width: 93px;
	height: 79px;
	background: url(../../img/onOff_03.png) no-repeat;
}
.send_message{
	color: #89b1ef; 
}
/*点击后展示*/
.r_show{
	display: block !important;
}
